[
    {
        "taskName": "Books",
        "localFolder": "C:\\Users\\username\\Books",
        "destName": "MyOneDrive",
        "destFolder": "/Backups/Books",
        "exclude": [
            "/*.txt",
            "/.git/"
        ],
        "rcloneFlags": "--dry-run --progress --fast-list --transfers=8 --max-backlog=-1 --log-level=NOTICE",
        "showCommand": true,
        "maximumLogFiles": 15,
        "enabled": true
    },
    {
        "taskName": "Music",
        "localFolder": "C:\\Users\\username\\Music",
        "destName": "MyDropbox",
        "destFolder": "/Backups/Music",
        "exclude": [
            "/*.cue",
            "/.git/"
        ],
        "rcloneFlags": "--dry-run --progress --fast-list --transfers=8 --max-backlog=-1 --log-level=NOTICE",
        "showCommand": true,
        "maximumLogFiles": 15,
        "enabled": true
    },
    {
        "taskName": "Photos",
        "localFolder": "C:\\Users\\username\\Photos",
        "destName": "MyGoogleDrive",
        "destFolder": "/Backups/Photos",
        "exclude": [
            "*.webp",
            "/.git/"
        ],
        "rcloneFlags": "--dry-run --progress --fast-list --transfers=8 --max-backlog=-1 --log-level=NOTICE",
